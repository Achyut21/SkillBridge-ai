{
  "name": "skillbridge-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3004",
    "dev:default": "next dev",
    "build": "prisma generate && next build",
    "build:prod": "prisma generate --no-engine && next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:generate:prod": "prisma generate --no-engine",
    "postinstall": "prisma generate",
    "check": "./scripts/check-system.sh",
    "clean": "rm -rf .next node_modules",
    "clean:install": "npm run clean && npm install",
    "test:api": "curl http://localhost:3004/api/config-check | python3 -m json.tool"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.10.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.298.0",
    "next": "^15.3.4",
    "next-auth": "^4.24.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.48.2",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "recharts": "^2.10.4",
    "sonner": "^2.0.5",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.19.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.30",
    "postcss": "^8",
    "prisma": "^6.10.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
